{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "no-unused-expression": false,
        "no-console": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format"
        ]
	}
}